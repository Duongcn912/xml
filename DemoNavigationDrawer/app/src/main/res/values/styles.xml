<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        
    </style>
    <style name="changecolorDrawerToggle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@color/colorWhite</item>
    </style>
    <style name="menuitemcolor" parent="AppTheme">
        <item name="drawerArrowStyle">@style/changecolorDrawerToggle</item>
        <item name="colorPrimary">#000</item>
        <item name="android:textColorPrimary">@color/colorWhite</item>

    </style>
    <style name="TabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textStyle">bold</item>
    </style>


</resources>
